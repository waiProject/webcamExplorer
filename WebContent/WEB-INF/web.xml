  </servlet>
  <servlet-mapping>
    <servlet-name>Picture</servlet-name>
    <url-pattern>/Picture</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>Date</display-name>
    <servlet-name>Date</servlet-name>
